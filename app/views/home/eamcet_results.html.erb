<%= render :partial => 'common/eamcet_search',  :locals => {:pdata => @data} %>
	<div class='results'>
	<ul>
	<% @results.each do |result|%>
		<li>
		<%= link_to result.college.name, college_path(result.college) %> - <%= result.course.name %>
		</li>
	<% end %>
	</ul>
	</div>
<%= will_paginate @results, :params => params[:eamcet] %>