<div class='news widget'>	
	<h2>Latest Eamcet News</h2>
	<div class='content'>	
	<ul>		
	<% @news_items.each do |news| %>
		<li>
			<div class='newst'><a href="<%= news.url %>" target='_blank'><%= news.description %></a></div>	
			<div class='source'><i>Source: <%= news.source %> | Date: <%= news.date %></i></div>				
		</li>
	<% end %>
	</ul>	
	</div>
</div>