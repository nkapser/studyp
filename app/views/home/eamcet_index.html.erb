<% content_for :rightside do %>
	<%= render :partial => 'common/college_search'%>	
	<%= render :partial => 'common/explore_colleges'%>
	<%= render :partial => 'common/ads/college_page'%>
<% end %>
<%= render :partial => 'common/eamcet_search', :locals => {:pdata => nil} %>

<% if RAILS_ENV == 'production' %>
<div style='margin-top: 15px'>
<script type="text/javascript"><!--
google_ad_client = "ca-pub-7537769370297267";
/* Text_Horizontal */
google_ad_slot = "2726197261";
google_ad_width = 728;
google_ad_height = 90;
//-->
</script>
<script type="text/javascript"
src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
</script>	
