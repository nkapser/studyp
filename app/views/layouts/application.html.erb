<!DOCTYPE html>
<html>
<head>
  <title>Studyp</title>

  <%= stylesheet_link_tag "style.css" %>
  <link type="text/css" href="/stylesheets/css/smoothness/jquery-ui-1.8.13.custom.css" rel="stylesheet" />	
  <%= javascript_include_tag "jquery/jquery-1.6.1.min.js", "jquery/jquery-ui-1.8.13.custom.min.js", "application.js"%>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id='header'>
		<div id='header_content'>
			<div id='logo'><a href="/"><img src='/images/logo.jpg' alt="Eamcet Colleges"/></a></div>
			<div class='g_ad'>
				<%= render :partial => 'common/ads/header_728x90'%>
			</div>
		</div>
	</div>
	<div id='container'>
		<div id='title'>
			<%= yield :title_text %>
		</div>
		
		<div id='page'>		
		
		<div id='content'>		
			<%= yield %>
		</div>
			
		<div id='rightside'>	
		<%= yield :rightside %>			
		</div>
		</div>
	</div>

	<div id='footer'>
	<div class='content'>	
		<ul>
			<li><%= link_to 'Home', '/'%></li>
			<li><%= link_to 'About Us'%></li> 
			<li><%= link_to 'Contact Us', '/contactus'%></li>
		</ul>
		<ul>
			<li><%= link_to 'Andhra Colleges', colleges_search_path(:region => "AU")%></li>
			<li><%= link_to 'Osmania Colleges',colleges_search_path(:region => "OU")%></li> 
			<li><%= link_to 'Sri Venkateswara Colleges', colleges_search_path(:region => "SVU")%></li>
		</ul>		
	</div>
	</div>
</body>
</html>
